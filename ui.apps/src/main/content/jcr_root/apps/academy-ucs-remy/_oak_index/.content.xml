<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:oak="http://jackrabbit.apache.org/oak/ns/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:rep="internal"
    jcr:mixinTypes="[rep:AccessControllable]"
    jcr:primaryType="nt:unstructured">

    <academy-ucs-remy-hashtag-index
        jcr:primaryType="oak:QueryIndexDefinition"
        async="[async,nrt]"
        compatVersion="{Long}2"
        evaluatePathRestrictions="{Boolean}true"
        reindex="{Boolean}false"
        reindexCount="{Long}3"
        seed="{Long}-4558802109986487089"
        includedPaths="{Path}[/content/academy-ucs-remy]"
        type="lucene">
        <indexRules jcr:primaryType="nt:unstructured">
            <cq:Page jcr:primaryType="nt:unstructured">
                <properties jcr:primaryType="nt:unstructured">
                    <template
                        jcr:primaryType="nt:unstructured"
                        name="jcr:content/cq:template"
                        propertyIndex="{Boolean}true"/>
                    <date
                        jcr:primaryType="nt:unstructured"
                        name="jcr:content/date"
                        ordered="{Boolean}true"/>
                    <tags
                        jcr:primaryType="nt:unstructured"
                        name="jcr:content/cq:tags"
                        propertyIndex="{Boolean}true"/>
                </properties>
            </cq:Page>
        </indexRules>
    </academy-ucs-remy-hashtag-index>

</jcr:root>
